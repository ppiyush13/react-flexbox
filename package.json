{
  "name": "react-flexbox",
  "version": "1.0.0",
  "description": "Simple, light, unopionated, css standard compliant Flexbox component for React",
  "main": "dist/react-flexbox.cjs.js",
  "module": "dist/react-flexbox.esm.js",
  "scripts": {
    "build": "rollup --config",
    "build:watch": "npm run build -- -w",
    "test": "jest --coverage --runInBand",
    "test:ci": "jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint . --ext .js,.jsx,.json",
    "codecov": "codecov",
    "test:precommit": "jest --passWithNoTests --bail --findRelatedTests",
    "lint:precommit": "eslint --ext .js,.jsx,.json"
  },
  "keywords": [
    "react",
    "styled-componenets",
    "styled",
    "flexbox",
    "flex"
  ],
  "author": {
    "name": "Piyush Lodaya",
    "email": "ppiyush13@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.1.0",
    "babel-plugin-styled-components": "1.10.6",
    "codecov": "^3.6.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-react": "^7.15.1",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^5.0.0-alpha.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "styled-components": "^4.3.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0",

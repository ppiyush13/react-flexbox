{
  "name": "react-styled-flex",
  "version": "1.0.17",
  "description": "Simple, light, unopionated, css standard compliant Flexbox component for React using Styled Components",
  "main": "dist/react-styled-flex.cjs.js",
  "module": "dist/react-styled-flex.esm.js",
  "scripts": {
    "build": "rimraf dist && rollup --config",
    "build:watch": "npm run build -- -w",
    "codecov": "codecov",
    "lint": "eslint . --ext .js,.jsx,.json",
    "lint:precommit": "eslint --ext .js,.jsx,.json",
    "lint:md": "remark .",
    "release": "npm run build && np --no-yarn --no-cleanup --no-2fa",
    "test": "jest --coverage --runInBand",
    "test:ci": "jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit",
    "test:precommit": "jest --passWithNoTests --bail --findRelatedTests"
  },
  "author": {
    "name": "Piyush Lodaya",
    "email": "ppiyush13@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ppiyush13/react-styled-flex.git"
  },
  "bugs": {
    "url": "https://github.com/ppiyush13/react-styled-flex/issues"
  },
  "homepage": "https://github.com/ppiyush13/react-styled-flex",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@testing-library/jest-dom": "4.2.3",
    "babel-plugin-styled-components": "1.10.6",
    "codecov": "^3.6.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^9.4.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.5",
    "remark-lint-heading-increment": "^1.0.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^5.0.0-alpha.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.2",
    "styled-components": "^4.4.1"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "styled-components": ">=4.0.0"
  },
  "keywords": [
    "react-styled-flex",
    "react",
    "styled-componenets",
    "styled",
    "flexbox",
    "flex",
    "react-flex",
    "styled-flex",
    "react-flexbox",
    "styled-flexbox",
    "flex-gap",
    "gap"
  ]
}
